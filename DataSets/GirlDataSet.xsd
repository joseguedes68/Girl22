                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT     Armazens.ArmAbrev AS Armazem, DocCab.DataDoc AS Data, TipoDoc.TipoDocDesc AS TipoDocumento, DocCab.DocNr AS NºDoc, 
                      Armazens_1.ArmAbrev AS Terceiro, DocCab.DocNrOrig, SUM(DocDet.Qtd * TipoDoc.MovStock) AS QtdDoc, 0 AS Stock, DocCab.ArmazemID, 
                      DocCab.TipoDocID
FROM         DocCab INNER JOIN
                      DocDet ON DocCab.EmpresaID = DocDet.EmpresaID AND DocCab.ArmazemID = DocDet.ArmazemID AND DocCab.TipoDocID = DocDet.TipoDocID AND 
                      DocCab.DocNr = DocDet.DocNr INNER JOIN
                      TipoDoc ON DocCab.TipoDocID = TipoDoc.TipoDocID INNER JOIN
                      Armazens ON DocCab.ArmazemID = Armazens.ArmazemID LEFT OUTER JOIN
                      Armazens AS Armazens_1 ON DocCab.TercID = Armazens_1.ArmazemID
WHERE     (DocCab.ArmazemID = @Armz) AND (TipoDoc.MovStock &lt;&gt; 0) AND (DocCab.DataDoc &gt;= @DeData) AND (DocCab.DataDoc &lt;= @AteData) AND 
                      (DocCab.TipoDocID + DocCab.EstadoDoc &lt;&gt; 'SEC')
GROUP BY TipoDoc.TipoDocDesc, DocCab.DocNr, Armazens.ArmAbrev, Armazens_1.ArmAbrev, DocCab.DocNrOrig, DocCab.DataDoc, DocCab.DataDoc, 
                      DocCab.ArmazemID, DocCab.TipoDocID
ORDER BY Armazem, Data, NºDoc</CommandText>
